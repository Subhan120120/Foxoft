<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Migrations\New folder\**" />
    <EmbeddedResource Remove="Migrations\New folder\**" />
    <None Remove="Migrations\New folder\**" />
    <Page Remove="Migrations\New folder\**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="Properties\AssemblyInfo.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="AppCode\GUNSONU.repx" />
    <None Remove="AppCode\GvListDefaultLayout.xml" />
    <None Remove="AppCode\Qry_Customers.sql" />
    <None Remove="AppCode\Qry_Expences.sql" />
    <None Remove="AppCode\Qry_Payments.sql" />
    <None Remove="AppCode\Qry_Sales.sql" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="Migrations\New folder\subContextModelSnapshot.cs" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="AppCode\GUNSONU.repx" />
    <EmbeddedResource Include="AppCode\GvListDefaultLayout.xml" />
    <EmbeddedResource Include="AppCode\Qry_Expences.sql" />
    <EmbeddedResource Include="AppCode\Qry_Payments.sql" />
    <EmbeddedResource Include="AppCode\Qry_Customers.sql" />
    <EmbeddedResource Include="AppCode\Qry_Sales.sql" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="DevExpress.Win.Design" Version="21.2.*-*" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="5.0.14" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Tools" Version="5.0.14">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
    <Compile Update="Properties\Settings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>PublicSettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>